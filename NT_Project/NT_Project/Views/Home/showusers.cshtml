@model IEnumerable<NT_Project.Models.ApplicationUser>

@{
    ViewBag.Title = "showusers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>showusers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Photo_Url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
       
        
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.Photo_Url)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
       
        
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "DeleteUser", new { id=item.Id })
        </td>
    </tr>
}

</table>
